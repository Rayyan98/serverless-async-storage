{
  "name": "serverless-async-storage",
  "version": "1.1.0",
  "description": "Ensure separate contexts for consecutive warm lambda executions",
  "main": "dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./plugin": {
      "import": "./dist/plugin.js",
      "require": "./dist/plugin.js"
    }
  },
  "scripts": {
